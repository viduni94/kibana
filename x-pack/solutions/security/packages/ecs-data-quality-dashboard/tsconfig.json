    "types": [
      "@kbn/ambient-ui-types",
      "jest",
      "node",
      "react"
    ]
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["target/**/*"],
  "kbn_references": [
    "@kbn/i18n",
