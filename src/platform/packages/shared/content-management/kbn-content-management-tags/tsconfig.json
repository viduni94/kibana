{
  "extends": "@kbn/tsconfig-base/tsconfig.json",
  "compilerOptions": {
    "outDir": "target/types",
    "types": [
      "@kbn/ambient-ui-types",
      "jest",
      "node",
      "react"
    ]
  },
  "include": [
    "index.ts",
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "target/**/*"
  ],
  "kbn_references": [
    "@kbn/i18n",
    "@kbn/saved-objects-tagging-oss-plugin",
    "@kbn/core-notifications-browser",
    "@kbn/shared-ux-utility"
  ]
}
