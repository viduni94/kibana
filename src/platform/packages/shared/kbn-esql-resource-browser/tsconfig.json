{
  "extends": "@kbn/tsconfig-base/tsconfig.json",
  "compilerOptions": {
    "outDir": "target/types",
    "types": [
      "jest",
      "node",
      "react",
      "@kbn/ambient-ui-types",
      "@kbn/ambient-storybook-types",
      "@testing-library/jest-dom",
      "@testing-library/react"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "kbn_references": [
    "@kbn/i18n",
    "@kbn/esql-types",
    "@kbn/react-field",
    "@kbn/field-utils"
  ],
  "exclude": [
    "target/**/*",
    ".storybook/**/*"
  ]
}
