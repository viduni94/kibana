{
  "extends": "@kbn/tsconfig-base/tsconfig.json",
  "compilerOptions": {
    "outDir": "target/types",
    "types": [
      "jest",
      "node",
      "react",
      "@kbn/ambient-ui-types",
      "@testing-library/jest-dom",
      "@testing-library/react"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "target/**/*"
  ],
  "kbn_references": [
    "@kbn/i18n",
    "@kbn/expressions-plugin",
    "@kbn/css-utils",
    "@kbn/chart-expressions-common",
    "@kbn/core",
    "@kbn/safer-lodash-set"
  ]
}
